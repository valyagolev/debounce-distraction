{
    "manifest_version": 2,
    "name": "Distraction Debounce",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": ["*://*/*"],
            "js": ["contentScript.js"],
            "run_at": "document_start"
        }
    ],
    "permissions": [
        "storage"
    ],
    "web_accessible_resources": [
        "page.html",
        "pageScript.js"
    ]
  }